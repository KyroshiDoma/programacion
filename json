#include <iostream>
#include <Windows.h>
#include <nlohmann/json.hpp>

usinf json=nlohmann::json;
using namespace std;

int main(){
	json j;
	j["pi"]=3.141592;
	j["feliz"]=true;
	j["nombre"]="pedro";
	j["vacio"]=nullptr;
	j["nuevo"]["valor"]=25;
	j["lista"]={1,0,3};
	j["nuevo"]["olista"]={0,1,2,3};
	j["objeto"]={{"moneda","USD"},{"valor"}};
	cout<<j<<endl;

	json j2=[{"pi",3.141592},
		{"feliz",true},
		{"nombre","pedro"},
		{"vacio",nullptr},
		{"nuevo",{{"valor",25}},
		{"lista",{1,0,3}},
		{"objeto",{{"moneda","USD"},{"valor"}}
	];

	cout<<j2<<endl;
}
